    "def clear_num(num):\n",
    "    \"\"\"\n",
    "    Очистка id корневой сделки и перевод ео в int\n",
    "    \"\"\"\n",
    "    if num.isdigit():\n",
    "        return int(num)\n",
    "    else:\n",
    "        l = len(num)-1\n",
    "        while l>0:\n",
    "            if num[l].isdigit():\n",
    "                l -= 1\n",
    "            else:\n",
    "                break\n",
    "        return int(num[l+1:])"
   ]
  },
