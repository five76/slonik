   "source": [
    "Отделение prefix от number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 92,
   "id": "36fea7c4",
   "metadata": {},
   "outputs": [],
   "source": [
    "def separate_prefix_number(row):\n",
    "    pl = all_prefix\n",
    "    \n",
    "    v = row['code']\n",
    "    if row['prefix'] == '':\n",
    "        amo_id = row['amo_id']\n",
    "        \n",
    "        for el in pl:\n",
    "            if el in row['code']:\n",
    "                try:\n",
    "                    v = el + '_' + row['code'][len(el):] \n",
    "                except:\n",
    "                    pass\n",
    "                finally:\n",
    "                    return v\n",
    "        \n",
    "        pl = prefix_amo        \n",
    "        for el in all_prefix:\n",
    "            if el in row['code']:\n",
    "                try:\n",
    "                    v = el + '_' + row['code'][len(el):] \n",
    "                except:\n",
    "                    pass\n",
    "                finally:\n",
    "                    return v\n",
    "    return v"
   ]
  },
