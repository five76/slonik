    "При сортировке обнаружилась кириллица в префиксах"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "01987e14",
   "metadata": {},
   "outputs": [],
   "source": [
    "alf_ciril = 'Е Т О Р А Н К Х С В М'.split()\n",
    "alf_latin = 'E T O P A H K X C B M'.split()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "caf6dccf",
   "metadata": {},
   "outputs": [],
   "source": [
    "def replace_cirillic_latin(x):\n",
    "    v = x\n",
    "    new_v = ''\n",
    "    for i in range(len(v)):\n",
    "        simv = v[i]\n",
    "        if simv in alf_ciril:\n",
    "            pos = alf_ciril.index(simv)\n",
    "            simv = alf_latin[pos]\n",
    "        new_v += simv\n",
    "    return new_v"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "2f6091f3",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_archive['prefix'] = df_archive.apply(lambda x: replace_cirillic_latin(x['prefix']),axis=1)"
   ]
  },
